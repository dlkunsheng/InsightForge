<Window x:Class="InsightForge.Playground.ContainerControls.DockPanelDemo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InsightForge.Playground.ContainerControls"
        mc:Ignorable="d"
        Title="DockPanelDemo" Height="450" Width="800">
    <Grid>
        <TextBlock>DockPanel Demo</TextBlock>
        <DockPanel>
            <Button Content="Top" DockPanel.Dock="Top"/>
            <Button Content="Bottom" DockPanel.Dock="Bottom"/>
            <Button Content="Left" DockPanel.Dock="Left"/>
            <Button Content="Fill Content"/>
        </DockPanel>
    </Grid>
</Window>

    